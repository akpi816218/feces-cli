{
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"module": "ESNext",
		"moduleResolution": "nodenext",
		"outDir": "dist",
		"strict": true,
		"skipLibCheck": false,
		"target": "ESNext",
		"lib": [
			"ESNext"
		],
	},
	"include": [
		"src/**/*"
	],
}